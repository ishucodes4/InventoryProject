<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowInvoiceActivity">

    <TextView
        android:id="@+id/tvName"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="206dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvEmail"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvName" />

    <TextView
        android:id="@+id/tvVehicleId"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvEmail" />

    <TextView
        android:id="@+id/tvSlotId"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvVehicleId" />

    <TextView
        android:id="@+id/tvPaymentId"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSlotId" />

    <Button
        android:id="@+id/btnGoHome"

        android:layout_width="367dp"
        android:layout_height="56dp"
        android:layout_marginBottom="120dp"
        android:background="@drawable/custom_button"
        android:text="Go Home"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnPrint"
        android:layout_width="367dp"
        android:layout_height="56dp"
        android:layout_marginBottom="50dp"
        android:background="@drawable/custom_button"
        android:text="Print"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvInvoiceDate"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPaymentId" />

    <TextView
        android:id="@+id/tvInvoiceDuration"
        android:layout_width="367dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="17sp"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInvoiceDate" />

    <TextView
        android:id="@+id/textView7"
        android:layout_height="80dp"
        android:layout_width="367dp"
        android:layout_marginBottom="600dp"
        android:fontFamily="@font/butler_bold"
        android:textSize="30dp"
        android:textColor="@color/black"
        android:text="Your Order has been \nBooked!"
        android:lineSpacingExtra="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>